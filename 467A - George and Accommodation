import java.util.*;
import java.lang.*;
import java.io.*;
 

   class TestClass {
    public static void main(String args[] ) throws Exception {
    	Scanner sc = new Scanner(System.in);
        
        int n=sc.nextInt();
        int c=0;
        while(n>0)
        {
        	n--;
        	int p = sc.nextInt();
        	int l= sc.nextInt();
        	
        
        	if(l-p >= 2)
        	c++;
        }
        System.out.println(c);
        
    }
}
